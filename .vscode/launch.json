  "version": "0.2.0",
  "configurations": [
    {
      "name": "pnpm dev",
      "request": "launch",
      "runtimeArgs": [
        "dev"
      ],
      "runtimeExecutable": "pnpm",
      "runtimeVersion": "21.6.2",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ],
      "type": "node",
    },
    {
      "name": "Next.js: debug client-side",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Next.js: debug server-side",
      "request": "launch",
      "type": "node-terminal",
      "command": "pnpm run dev",
      "skipFiles": [
        "<node_internals>/**"
      ],
    }]
